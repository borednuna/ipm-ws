<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ipm_image_node</name>
  <version>0.0.0</version>
  <description>Inverse Perspective Mapping Node for Image or Mask Topics</description>
  <maintainer email="git@flova.de">Florian Vahl</maintainer>
  <license>Apache License 2.0</license>

  <depend>cv_bridge</depend>
  <depend>ipm_library</depend>
  <depend>python3-numpy</depend>
  <depend>python3-opencv</depend>
  <depend>rclpy</depend>
  <depend>sensor_msgs_py</depend>
  <depend>sensor_msgs</depend>
  <depend>tf2</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>geometry_msgs</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
